@model ServiceGateway.Models.Crew

@{
    ViewBag.Title = "Delete";
}

<h2>Delete Game</h2>

<h2>Are you sure you want to delete <strong>@Model.Name</strong>?</h2>
<div>
    <hr />
    <dl class="container itemContainer">
        <br />

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <dd>
                    <div class="form-actions no-color moveRight50" align="center">
                        <input type="submit" value="Delete" class="btn btn-success" />
                        @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-danger" })
                    </div>
                </dd>
            }

        <br />
    </dl>



</div>
